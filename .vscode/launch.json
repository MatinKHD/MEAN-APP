  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Angular",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: start",
      "url": "http://localhost:4200/",
      "webRoot": "${workspaceFolder}"
    },

    {
      "name": "ng serve",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: start",
      "url": "http://localhost:4200/"
    },
    {
      "command": "npm run start-server",
      "name": "start server",
      "request": "launch",
      "type": "node-terminal"
    }
  ]
}
